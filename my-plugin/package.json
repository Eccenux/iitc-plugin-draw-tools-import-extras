{
	"name": "iitc-plugin-draw-tools-import-extras",
	"version": "0.0.1",
	"description": "[0.0.1] Importing portal list (lat,lon list) to draw tools.",
	"author": "Eccenux",
	"license": "MIT",
	"main": "index.js",
	"monkeyscript": {
		"meta": {
			"name": "IITC plugin: Import extras for Draw Tools",
			"category": "Misc",
			"id": "iitc-plugin-draw-tools-import-extras",
			"namespace": "pl.enux.iitc",
			"grant": [
				"none"
			],
			"match": [
				"https://*.ingress.com/intel*",
				"http://*.ingress.com/intel*",
				"https://*.ingress.com/mission/*",
				"http://*.ingress.com/mission/*",
				"https://intel.ingress.com/*",
				"https://intel.ingress.com/*"
			]
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"eslint": "6.8.0",
		"gulp": "4.0.2",
		"gulp-concat": "2.6.1",
		"gulp-less": "4.0.1",
		"gulp-strip-comments": "2.5.2",
		"gulp-monkeyscript": "file:../../gulp-monkeyscript"
	}
}
